{
    "manifest_version": 2,

    "name": "Directangular Prism",
    "description": "This extension augments various Directangular web sites with debug info, etc.",
    "version": "1.2",

    "content_scripts": [{
        "matches": [
            "https://shoptheroe.com/*",
            "https://popitup.com/*",
            "https://sonlet.com/*",
            "https://beta.shoptheroe.com/*",
            "https://beta.popitup.com/*",
            "https://beta.sonlet.com/*"
        ],
        "js": ["jquery-3.2.1.min.js", "main.js"]
    }]
}
